{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "aditya-data-factory-1"
		},
		"linktoblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linktoblob'"
		},
		"linktosqlDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linktosqlDB'"
		},
		"eventTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/f8c21ef5-1827-4a74-a263-aad531215eb4/resourceGroups/aditya_resource_group_1/providers/Microsoft.Storage/storageAccounts/adityarajorastorage1"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/f8c21ef5-1827-4a74-a263-aad531215eb4/resourceGroups/aditya_resource_group_1/providers/Microsoft.Storage/storageAccounts/adityarajorastorage1"
		},
		"MyTrigger_properties_pipeline1_parameters_FileName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"MyTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/f8c21ef5-1827-4a74-a263-aad531215eb4/resourceGroups/aditya_resource_group_1/providers/Microsoft.Storage/storageAccounts/adityarajorastorage1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "aditya-data-factory-1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linktoblob_connectionString": {
						"value": "[parameters('linktoblob_connectionString')]"
					},
					"linktosqlDB_connectionString": {
						"value": "[parameters('linktosqlDB_connectionString')]"
					},
					"eventTrigger_properties_typeProperties_scope": {
						"value": "[parameters('eventTrigger_properties_typeProperties_scope')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					},
					"MyTrigger_properties_pipeline1_parameters_FileName": {
						"value": "[parameters('MyTrigger_properties_pipeline1_parameters_FileName')]"
					},
					"MyTrigger_properties_typeProperties_scope": {
						"value": "[parameters('MyTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}